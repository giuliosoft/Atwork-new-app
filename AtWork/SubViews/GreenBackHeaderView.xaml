<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AtWork.SubViews.GreenBackHeaderView">
    <ContentView.Content>
        <StackLayout WidthRequest="100">
            <Image x:Name="headerbackgreen" Source="headerbackgreen" HorizontalOptions="Start" Margin="20,10,10,10" />
            <StackLayout.GestureRecognizers>
                <TapGestureRecognizer Command="{TemplateBinding BindingContext.GreenHeaderViewBackCommand}"/>
            </StackLayout.GestureRecognizers>
        </StackLayout>
    </ContentView.Content>
</ContentView>
